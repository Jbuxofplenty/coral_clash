{
    "expo": {
        "name": "Coral Clash",
        "description": "Coral Clash Board Game",
        "slug": "coral-clash",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./src/assets/icons/app_icon.png",
        "splash": {
            "image": "./src/assets/images/splash.png",
            "resizeMode": "contain"
        },
        "updates": {
            "fallbackToCacheTimeout": 0,
            "url": "https://u.expo.dev/416801e2-5620-4693-8729-161decbd0b5b"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "googleServicesFile": "./GoogleService-Info.plist",
            "supportsTablet": true,
            "bundleIdentifier": "com.jbuxofplenty.coralclash",
            "runtimeVersion": "1.0.0",
            "appleTeamId": "FWV22U8U39",
            "associatedDomains": ["webcredentials:coral-clash.firebaseapp.com"],
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false,
                "CFBundleURLTypes": [
                    {
                        "CFBundleURLSchemes": ["coralclash"]
                    }
                ]
            }
        },
        "plugins": ["expo-font", "@react-native-google-signin/google-signin"],
        "android": {
            "googleServicesFile": "./google-services.json",
            "package": "com.jbuxofplenty.coralclash",
            "runtimeVersion": {
                "policy": "appVersion"
            },
            "intentFilters": [
                {
                    "action": "VIEW",
                    "autoVerify": true,
                    "data": [
                        {
                            "scheme": "https",
                            "host": "coral-clash.firebaseapp.com"
                        }
                    ],
                    "category": ["BROWSABLE", "DEFAULT"]
                }
            ]
        },
        "extra": {
            "eas": {
                "projectId": "416801e2-5620-4693-8729-161decbd0b5b"
            }
        }
    }
}
