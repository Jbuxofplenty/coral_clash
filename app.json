{
    "expo": {
        "name": "coral-clash",
        "description": "Coral Clash Board Game",
        "slug": "coral-clash",
        "privacy": "unlisted",
        "platforms": ["ios", "android"],
        "version": "1.8.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "splash": {
            "image": "./src/assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#1e3c72"
        },
        "backgroundColor": "#1e3c72",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.jbuxofplenty.coralclash"
        },
        "plugins": ["expo-font"],
        "android": {
            "package": "com.jbuxofplenty.coralclash"
        }
    }
}
